    },
    "cmake.configureOnOpen": true,
    "flake8.args": [
        "--ignore=CNL100",
        "--max-line-length=100",
    ],
    "ros.distro": "humble",
    "terminal.integrated.gpuAcceleration": "off",
    "cmake.debugConfig": {
        "args": [
            "${workspaceFolder}/config/example.yaml",
            "${workspaceFolder}/config/example/"
